{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "jest": true,
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": [
      "error", 
      "never"
    ],
    "no-unused-vars": [
      "error", {
        "vars": "local",
        "args": "none"
      }
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "react/jsx-filename-extension": 0
  }
}